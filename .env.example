# Copy to .env and fill
ANTHROPIC_API_KEY=

# Cartridge controller-cli
# Mainnet Game contract from docs (can override if needed)
LS2_CONTRACT_ADDRESS=0x6f7c4350d6d5ee926b3ac4fa0c9c351055456e75c92227468d84232fc493a9c
ADVENTURER_ID=

# Safety: refuse to run if estimated wallet balance exceeds this (USD)
MAX_USD_BALANCE=10

# Runner tuning
TICK_INTERVAL_MS=10000
MAX_TICKS=500
MAX_CONSECUTIVE_ERRORS=5
MAX_TX_PER_MINUTE=6
MAX_TX_PER_SESSION=200

# Browser is read-only sensor; can be disabled
USE_BROWSER=true
HEADLESS=true
